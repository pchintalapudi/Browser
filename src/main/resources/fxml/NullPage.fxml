<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.geometry.Point3D?>
<?import javafx.scene.Group?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.paint.PhongMaterial?>
<?import javafx.scene.shape.Box?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.transform.Rotate?>

<StackPane fx:id="root" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pc.browser.resources.NullPageController">
    <children>
        <VBox alignment="CENTER" spacing="30.0">
            <children>
                <Group>
                    <children>
                        <Group>
                            <children>
                                <Box cache="true" cacheHint="ROTATE" depth="100.0" height="100.0" width="100.0">
                                    <transforms>
                                        <Rotate fx:id="yRotate" angle="0.0" pivotX="0.0" pivotY="0.0" pivotZ="0.0">
                                            <axis>
                                                <Point3D x="0.0" y="1.0" z="0.0" />
                                            </axis>
                                        </Rotate>
                                        <Rotate angle="35.26" pivotX="0.0" pivotY="0.0" pivotZ="0.0">
                                            <axis>
                                                <Point3D x="0.0" y="0.0" z="1.0" />
                                            </axis>
                                        </Rotate>
                                        <Rotate angle="45.0" pivotX="0.0" pivotY="0.0" pivotZ="0.0">
                                            <axis>
                                                <Point3D x="1.0" y="0.0" z="0.0" />
                                            </axis>
                                        </Rotate>
                                    </transforms>
                                    <material>
                                        <PhongMaterial diffuseColor="#0088ff88" specularColor="#0088ff22" />
                                    </material>
                                </Box>
                            </children>
                        </Group>
                    </children>
                </Group>
                <Label text="You haven't loaded anything yet!">
                    <font>
                        <Font size="32.0" />
                    </font>
                </Label>
            </children>
            <padding>
                <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
            </padding>
        </VBox>
    </children>
</StackPane>
